<div class="page-header">
  <%= link_to involvements_path, :class => 'btn' do %>
    <i class="icon-list-alt"></i>
    Back
  <% end %>
  <%= link_to edit_involvement_path(@involvement), :class => 'btn btn-primary' do %>
    <i class="icon-pencil icon-white"></i>
    Edit
  <% end %>
  <h1>Listing involvements</h1>
</div>

<dl>
  <dt>Allow appointment:</dt>
  <dd><%= @involvement.allow_appointment %></dd>

  <dt>Price:</dt>
  <dd><%= @involvement.price %></dd>

  <dt>From:</dt>
  <dd><%= @involvement.from %></dd>

  <dt>To:</dt>
  <dd><%= @involvement.to %></dd>

  <dt>Hospital:</dt>
  <dd><%= @involvement.hospital %></dd>

</dl>
